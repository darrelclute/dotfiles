!Xterm*termName: xterm-256color
!URxvt*termName: rxvt-256color
Xft.lcdfilter: lcddefault
Xft.dpi:      96
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb
URxvt.scrollBar: false
URxvt*scrollBar_right: false
URxvt.font: xft:Hack:style=Regular:size=7:antialias=true,xft:PowerlineSymbols:style=Medium:size=7:antialias=true,xft:Source Code Pro:style=Regular:size=7:antialias=true,xft:Droid Sans:style=Regular:size=7:antialias=true
URxvt.loginShell: true
!URxvt.letterSpace: -4
! Background color
URxvt.background: black
! Font color
URxvt.foreground: green
! Other colors
URxvt.color0: black
URxvt.color1: red3
URxvt.color2: green3
URxvt.color3: yellow3
URxvt.color4: blue2
URxvt.color5: magenta3
URxvt.color6: cyan3
URxvt.color7: gray90
URxvt.color8: grey50
URxvt.color9: red
URxvt.color10: green
URxvt.color11: yellow
URxvt.color12: blue
URxvt.color13: magenta
URxvt.color14: cyan
URxvt.color15: white
URxvt.saveLines: 0
URxvt*internalBorder: 0
URxvt.tripleclickwords: true
URxvt.borderless: true
URxvt*skipBuiltinGlyphs: true
URxvt.perl-ext:
URxvt.perl-ext-common:
